<div class="aside" id="preregistration">
  <h5>Register for Updates</h5>
  <%= form_for Preregistration.new do |f| %>
    <%= f.email_field :email, :placeholder => "email address", :class => 'required email' %>
    <button type="submit" id="preregistration_submit" name="commit">ok</button>
  <% end %>
</div>
