<h4>Listing submissions</h4>

<table>
  <tr>
    <th>Email</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Title</th>
    <th>Abstract</th>
    <th>Home page</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @submissions.each do |submission| %>
  <tr>
    <td><%= submission.email %></td>
    <td><%= submission.first_name %></td>
    <td><%= submission.last_name %></td>
    <td><%= submission.title %></td>
    <td><%= submission.abstract %></td>
    <td><%= submission.home_page %></td>
    <td><%= link_to 'Show', submission %></td>
    <td><%= link_to 'Edit', edit_submission_path(submission) %></td>
    <td><%= link_to 'Destroy', submission, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Submission', new_submission_path %>
