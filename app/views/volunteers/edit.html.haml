%h1 Editing volunteer
- form_for(@volunteer) do |f|
  = f.error_messages
  %p
    = f.label :full_name
    %br/
    = f.text_field :full_name
  %p
    = f.label :email
    %br/
    = f.text_field :email
  %p
    = f.label :preferences
    %br/
    = f.text_area :preferences
  %p
    = f.submit 'Update'
= link_to 'Show', @volunteer
|
\#{link_to 'Back', volunteers_path}
