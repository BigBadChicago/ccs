<h4>Listing preregistrations</h4>

<table>
  <tr>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @preregistrations.each do |preregistration| %>
  <tr>
    <td><%= preregistration.email %></td>
    <td><%= link_to 'Show', preregistration %></td>
    <td><%= link_to 'Edit', edit_preregistration_path(preregistration) %></td>
    <td><%= link_to 'Destroy', preregistration, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Preregistration', new_preregistration_path %>
